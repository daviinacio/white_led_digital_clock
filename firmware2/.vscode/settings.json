{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/CVS": true,
    "**/node_modules": true,
    ".vscode": true
  },
  "cSpell.words": [
    "ATMEGA",
    "COMPA",
    "CONFIGURACAO",
    "DDRB",
    "DDRD",
    "OCIE",
    "PORTB",
    "PORTD",
    "runned",
    "TCCR",
    "TIMSK",
    "vect",
    "WLDC"
  ],
  "C_Cpp.errorSquiggles": "enabled",
  "files.associations": {
    "*.ds_store": "cpp"
  },
  "cSpell.customDictionaries": {
    "custom-dictionary-workspace": {
      "name": "custom-dictionary-workspace",
      "path": "${workspaceFolder:firmware2}/.cspell/custom-dictionary-workspace.txt",
      "addWords": true,
      "scope": "workspace"
    }
  }
}
